{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ {
      "source": "/guide/resourceoptimization-billingmetrics{,/**}",
      "destination": "/guide/resource_optimization_billing_metrics",
      "type": 301
    },{
      "source": "/guide/resourceoptimization-performanceoptimization{,/**}",
      "destination": "/guide/resource_optimization_performance_optimization",
      "type": 301
    },{
      "source": "/guide/resourceoptimization-setupconfiguration{,/**}",
      "destination": "/guide/resource_optimization_setup",
      "type": 301
    },{
      "source": "/guide/resourceoptimization-usagemonitoring{,/**}",
      "destination": "/guide/resource_optimization_usage_monitoring",
      "type": 301
    },{
      "source": "/guide/Auto-Ingest_Twitter_Data_into_Snowflake{,/**}",
      "destination": "/guide/auto_ingest_twitter_data",
      "type": 301
    } ]
  }
}
